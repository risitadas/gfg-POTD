

class Solution {
    public static void convertToWave(int n, int[] a) 
    {
        // code here
        
        int dummy = 0;
        
        for(int i=0; i<n-1; i = i+2)
        {
            dummy = a[i];
            a[i] = a[i+1];
            a[i+1] = dummy;
            
        }
        
        
     
        
    }
}
        
