
// User function Template for Java

class Solution {
    int sumOfDependencies(ArrayList<ArrayList<Integer>> adj, int V) 
    {
        // code here
        
        
        int result = 0;
        for (int i = 0; i < V; i++) 
        {
            
            
            result += adj.get(i).size();
        }
        return result;

    }
};
