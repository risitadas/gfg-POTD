

//User function Template for Java

class Solution {
     public int solve(String S, int n) 
     {
        // code here''
        
        
        HashSet<Character> hs=new HashSet<Character>();
        
		  for(int i=0;i<n;i++) 
		  {
			  hs.add(S.charAt(i));
			  
		  }
		  
		  return hs.size()-1;

        
        
        
        
    }
}
