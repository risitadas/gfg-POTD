

//User function Template for Java
class Solution 
{ 
    String construct(int N, int K) 
    { 
        // code here
        
        StringBuffer s = new StringBuffer("");
        
        for(int i=0;i<N;i++)
        {
            if(i%2==0)
            {
                s.append('a');
            }
            else
            {
                s.append('b');
            }
            
        }
        
        return s.toString();

        
        
    }
} 
